<section id="viewCatalog" class="background-img">
    {{#each electronics}}
    <div class="added-electronics">

        <a href="/electronic/details/{{_id}}" class="added-electronics-in-market">
            <img src="{{image}}" class="{{name}}">
            <h3>{{name}}</h3>
            <span>Price: ${{price}}</span>
        </a>

    </div>
    {{else}}
    <div class="guest">
        There are no electronics found...
    </div>
    {{/each}}
</section>